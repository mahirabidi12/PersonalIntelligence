# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/blinkit

# Frontend URL
FRONTEND_URL=http://localhost:5173

# JWT Secret Keys
SECRET_KEY_ACCESS_TOKEN=your_access_token_secret_key_here
SECRET_KEY_REFRESH_TOKEN=your_refresh_token_secret_key_here

# Resend API (for email)
RESEND_API=your_resend_api_key_here

# Cloudinary (for image uploads)
CLODINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLODINARY_API_KEY=your_cloudinary_api_key
CLODINARY_API_SECRET_KEY=your_cloudinary_api_secret

# Stripe (for payments)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_ENPOINT_WEBHOOK_SECRET_KEY=your_stripe_webhook_secret_here

# Server Port (optional, defaults to 8080)
PORT=3001